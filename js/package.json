{
  "name": "jupyterlab_email",
  "version": "0.2.4",
  "description": "Sending emails from JupyterLab",
  "author": "Tim Paine",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "scripts": {
    "build:babel": "babel src/ --source-maps --out-dir lib/",
    "build:lab": "jupyter labextension build .",
    "build": "npm-run-all build:*",
    "clean": "rimraf lib ../jupyterlab_email/labextension",
    "fix": "yarn lint --fix",
    "lint": "eslint -c .eslintrc.js --ext .js src/ tests/",
    "prepublishOnly": "npm run build",
    "test": "jest --coverage --collectCoverageFrom=src/*.{js}"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,svg}"
  ],
  "jupyterlab": {
    "extension": "lib/index.js",
    "outputDir": "../jupyterlab_email/labextension",
    "discovery": {
      "server": {
        "base": {
          "name": "jupyterlab_email"
        },
        "managers": [
          "pip"
        ]
      }
    }
  },
  "dependencies": {
    "@jupyterlab/application": "^3.6.1",
    "@jupyterlab/apputils": "^3.6.1",
    "@jupyterlab/coreutils": "^5.6.1",
    "@jupyterlab/docmanager": "^3.6.1",
    "@jupyterlab/filebrowser": "^3.6.1",
    "@jupyterlab/launcher": "^3.6.1",
    "@jupyterlab/mainmenu": "^3.6.1",
    "@jupyterlab/notebook": "^3.6.1",
    "@lumino/disposable": "^1.10.4",
    "requests-helper": "^0.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@jupyterlab/builder": "^3.6.1",
    "babel-jest": "^29.4.3",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "jest-transform-css": "^6.0.1",
    "mkdirp": "^2.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2"
  }
}
