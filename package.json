{
  "name": "jupyterlab_email",
  "version": "0.1.5",
  "description": "A JupyterLab extension.",
  "author": "Tim Paine",
  "main": "lib/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "lint": "./node_modules/.bin/tslint 'src/*.ts'",
    "prepublish": "npm run build",
    "test": "jest --coverage --collectCoverageFrom=src/*.{ts}"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.css"
  ],
  "jupyterlab": {
    "extension": true
  },
  "dependencies": {
    "@jupyterlab/application": "^0.19.0",
    "@jupyterlab/apputils": "^0.19.0",
    "@jupyterlab/coreutils": "^2.2.0",
    "@jupyterlab/docmanager": "^0.19.0",
    "@jupyterlab/filebrowser": "^0.19.0",
    "@jupyterlab/launcher": "^0.19.0",
    "@jupyterlab/mainmenu": "^0.8.0",
    "@phosphor/disposable": "^1.1.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "jest": "^23.6.0",
    "jest-transform-css": "^2.0.0",
    "rimraf": "^2.6.1",
    "ts-jest": "^23.10.4",
    "tslint": "^5.14.0",
    "typescript": "^3.1.6"
  }
}
